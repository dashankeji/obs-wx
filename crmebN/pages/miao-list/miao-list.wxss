page{
  background-color: #fff;
}
.wrap_imageOne{
  width: 100%;
  height: 280rpx;
  background-size:90% 90%;
  background-repeat: no-repeat;
  background-position: center;
}

.basis-df {
  flex-basis: 50%;
}

.justify-between {
  justify-content: space-between;
}


/*用来包裹所有的小圆点  */
.dots{  
  height: 15rpx; 
  display: flex;
  flex-direction: row;
  position: absolute;
  right: 0px;
  bottom: 5px;
}  
/*未选中时的小圆点样式 */
.dot{  
  width: 15rpx;  
  border-radius: 50%;
  margin-right: 26rpx;
  background-color: white;
}  
/*选中以后的小圆点样式  */
.active{  
  background-color:#ccc;
} 
.basis-sm {
flex-basis:46%;
}
.basis-lg{
  flex-basis:66%;
}
.basis-xxs{
  flex-basis:30%;
}

/*菜单和店家切换颜色*/
.tahSelectCss{  
   border-bottom:10rpx solid red;
   font-weight: bold;
} 

/*菜单按钮切换时的border*/
.border-l-red{  
   border-left: 16rpx solid red;
} 

.screen-swiper {
  min-height: 302rpx;
}

.screen-swiper image {
  width: 100%;
  display: block;
  height: 100%;
  margin: 0;
}
.wrap_imageTwo{
  width: 350rpx;
  height: 350rpx;
}
.foots-wrapper{position: absolute; left: 0; bottom: 0; width: 50%; height: 100rpx; line-height: 100rpx; text-align: center; background-color: #f8931f; color: #fff;border-radius:0;}
.foots-wrapper-two{position: absolute; right: 0; bottom: 0; width: 50%; height: 100rpx; line-height: 100rpx; text-align: center; background-color: #ff3d3d; color: #fff;border-radius:0;}
