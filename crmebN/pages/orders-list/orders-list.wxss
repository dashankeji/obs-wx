.head{position: relative; width: 100%; height: 100rpx; padding: 20rpx; box-sizing: border-box;}
.head input{width: 100%; height: 60rpx; padding-left: 68rpx; border-radius: 30px; background-color: #fff; border:1px solid #eee; box-sizing: border-box;}
.head .iconfont{position: absolute; left: 40rpx; top: 34rpx; color: #939393;}
.menu{background-color: #fff;  border-bottom: 1px solid #f7f7f7;}
.menu .item{width: 20%;height: 85rpx; line-height: 85rpx; text-align: center; box-sizing: border-box;}
.menu .item.on{color: #ff3d3d; border-bottom: 1px solid #ff3d3d;}
.orders-list .items{background-color: #fff;}
.orders-list .items .numbers{height: 80rpx; line-height: 80rpx; padding: 0 20rpx; border-bottom: 1px solid #f7f7f7;justify-content:space-between;align-items:center}
.del{font-size:40rpx!important;color:#CCCCCC!important;}
/*.orders-list .items .numbers text{color: #999;}*/
.pay-status{height: 100rpx;padding: 0 20rpx; border-bottom: 1px solid #f7f7f7; justify-content: space-between; align-items: center;}
.pay-status .status-txt{font-size: 26rpx; color: #999;} 
.pay-status .status-txt text{color: #ff3d3d;}
.pay-status .all-price{font-size: 26rpx; color: #e54d42; margin-top: 5rpx;}
.pay-status .all-price text{color: #e54d42;}
.pay-status .right-wrapper .buypay{width: 165rpx; height: 58rpx; line-height: 58rpx; text-align: center; background-color: #ff3d3d; color: #fff; border-radius: 15rpx;display:block;}
.pay-status .right-wrapper .buypay-two{width: 165rpx; height: 58rpx; line-height: 58rpx; text-align: center; color: #ccc; border-radius: 15rpx;display:block;border: 1px solid  #ccc;}
.buypays{width: 165rpx; height: 58rpx; line-height: 58rpx; text-align: center; border:1px solid #f7921e;color: #f7921e; border-radius: 3px;display:block;}
.info-list{padding: 20rpx;}
/*.info-list .pro-item{padding: 20rpx 20rpx 0; background-color: #fafafa; }*/
.info-list .pro-item .wrapper{width: 100%; justify-content: space-between; border-bottom: 1px dashed #ddd; padding-bottom: 20rpx;}
.info-list .pro-item image{width: 163rpx; height: 163rpx;}
.info-list .pro-item .item-info{width: 520rpx; justify-content: space-between;}
.info-list .pro-item .item-info .title{
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
  align-content: center;
  width: 92%;
  }
.info-list .pro-item .item-info .res{font-size: 26rpx; color: #999;}
.info-list .pro-item .item-info .price{
   color: #ff3d3d;
  font-size: 32rpx;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
  align-content: center;
  max-width: 40%;
}
.info-list .pro-item .item-info .price text{font-size: 24rpx;}
.info-list .pro-item .item-info .price .count{ color: #333; margin-left: 10rpx;}
.info-list .pro-item:last-child .wrapper{border-bottom: 0;}
.state-ytk{color:#333!important;}
.youfei{font-size:22rpx;}
.kuang{border:1rpx solid #999!important;background-color:#fff!important;color:#999!important;}
@import "/pages/home/home.wxss"; 